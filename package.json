{
  "name": "Angular-ES6-starter",
  "version": "0.0.2",
  "description": "SPA client application connecting to RESTful services. ",
  "author": "Kendrick Burson",
  "main": "index.html",
  "private": true,
  "analyze": true,
  "scripts": {
    "prerefresh": "gulp clean",
    "refresh": "git pull && git clean -i",
    "postrefresh": "npm install",
    "prereset": "echo '--- deleting all node and bower dependencies'",
    "reset": "rm -rf node_modules bower_components _build",
    "check-global": "npm-check -u -g",
    "check": "npm-check -u",
    "checker": "npm outdated --depth=0",
    "build": "gulp build",
    "watch": "gulp dev",
    "serve": "gulp serve",
    "postinstall": "bower install",
    "pretest": "gulp config.resources",
    "test": "gulp karma",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor client/src/tests/conf/protractor.conf.js",
    "start": "http-server _build -a localhost -p 8000 -c-1 -d false --cors",
    "express": "gulp express",
    "premock": "gulp build.mocked",
    "mock": "npm start",
    "preclient": "gulp build",
    "client": "npm run express",
    "prepublish": "echo '=== prepublish: run after npm install'",
    "publish": "echo 'publish: run after package is published'",
    "postpublish": "echo 'postpublish: run after package is published'"
  },
  "repository": {
    "type": "git",
    "url": "kburson@dvm-kburson1.sea.talentwisecorp.net:ver-client.git"
  },
